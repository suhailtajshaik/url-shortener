{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 10
      }
    }
  ],
  "routes": [
    {
      "src": "/api-docs.json",
      "dest": "/api/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api-docs",
      "dest": "/api/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
